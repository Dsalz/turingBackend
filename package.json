{
  "name": "turingbackend",
  "version": "1.0.0",
  "description": "Backend for app to be built for turing challenge",
  "main": "index.js",
  "scripts": {
    "start": " node ./dist",
    "dev": "nodemon --exec babel-node ./server",
    "test": "nyc mocha --require @babel/polyfill --require @babel/register ./server/test --timeout 20000 --exit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "rm -rf dist && mkdir dist && babel -d ./dist ./server -s",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "babel-eslint": "^10.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^6.0.2",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "stripe": "^6.34.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "codecov": "^3.3.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
